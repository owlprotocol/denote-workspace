{
    "$schema": "https://turborepo.org/schema.json",
    "globalEnv": [
        "DOTENV_KEY"
    ],
    "globalDependencies": [
        ".env.vault"
    ],
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "_cjs/**",
                "_esm/**",
                "_types/**"
            ]
        },
        "@denotecapital/minimal-token#build": {
          "dependsOn": ["^build"],
          "outputs": [".daml/**"]
        },
        "@denotecapital/token-app#build": {
          "dependsOn": ["^build"],
          "outputs": [".next/**"]
        },
        "depcheck": {
            "cache": false,
            "outputs": []
        },
        "syncpack": {
            "cache": false,
            "outputs": []
        },
        "madge": {
            "cache": false,
            "outputs": []
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "clean": {
            "cache": false
        },
        "start": {
            "cache": false,
            "persistent": true
        },
        "test": {
            "dependsOn": [
                "build",
                "^test"
            ],
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "lint:fix": {
            "dependsOn": [
                "^lint:fix"
            ],
            "outputs": []
        }
    }
}
